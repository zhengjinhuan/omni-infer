{
    "validator": {
        "model": {
            "validator_type": "supported"
        },
        "messages": {
            "validator_type": "supported",
            "subfield": ["messages.name", "messages.role", "messages.tool_call_id", "messages.content", 
                         "messages.prefix", "messages.refusal", "messages.partial", "messages.tool_calls", 
                         "messages.tool_calls.type", "messages.tool_calls.id", "messages.tool_calls.function", 
                         "messages.tool_calls.function.arguments", "messages.tool_calls.function.name"],
            "skip_subfield_check": ["messages.tool_calls.type"]
        },
        "stream": {
            "validator_type": "supported"
        },
        "stream_options": {
            "validator_type": "supported",
            "subfield": ["stream_options.include_usage", "stream_options.chunk_include_usage"]
        },
        "tool_choice": {
            "validator_type": "value",
            "target_value": ["auto"]
        },
        "tools": {
            "validator_type": "supported",
            "subfield": ["tools.type", "tools.function", "tools.function.description", "tools.function.name",
                         "tools.function.parameters", "tools.function.strict"],
            "skip_subfield_check": ["tools.function.parameters"]
        },
        "chat_template": {
            "validator_type": "supported"
        },
        "chat_template_kwargs": {
            "validator_type": "supported",
            "skip_subfield_check": ["chat_template_kwargs"]
        },
        "top_k": {
            "validator_type": "supported"
        },
        "top_p": {
            "validator_type": "supported"
        },
        "frequency_penalty": {
            "validator_type": "supported"
        },
        "presence_penalty": {
            "validator_type": "supported"
        },
        "temperature": {
            "validator_type": "supported"
        },
        "seed": {
            "validator_type": "supported"
        },
        "stop": {
            "validator_type": "supported",
            "skip_subfield_check": ["stop"]
        },
        "logit_bias": {
            "validator_type": "supported",
            "skip_subfield_check": ["logit_bias"]
        },
        "max_tokens": {
            "validator_type": "supported"
        },
        "add_generation_prompt": {
            "validator_type": "supported"
        },
        "add_special_tokens": {
            "validator_type": "supported"
        },
        "allowed_token_ids": {
            "validator_type": "supported",
            "skip_subfield_check": ["allowed_token_ids"]
        },
        "bad_words": {
            "validator_type": "supported",
            "skip_subfield_check": ["bad_words"]
        },
        "continue_final_message": {
            "validator_type": "supported"
        },
        "detokenize": {
            "validator_type": "supported"
        },
        "echo": {
            "validator_type": "supported"
        },
        "ignore_eos": {
            "validator_type": "supported"
        },
        "include_stop_str_in_output": {
            "validator_type": "supported"
        },
        "length_penalty": {
            "validator_type": "supported"
        },
        "max_completion_tokens": {
            "validator_type": "supported"
        },
        "min_p": {
            "validator_type": "supported"
        },
        "min_tokens": {
            "validator_type": "supported"
        },
        "prompt": {
            "validator_type": "supported"
        },
        "repetition_penalty": {
            "validator_type": "supported"
        },
        "request_id": {
            "validator_type": "supported"
        },
        "skip_special_tokens": {
            "validator_type": "supported"
        },
        "spaces_between_special_tokens": {
            "validator_type": "supported"
        },
        "stop_token_ids": {
            "validator_type": "supported",
            "skip_subfield_check": ["stop_token_ids"]
        },
        "enable_thinking": {
            "validator_type": "supported"
        }
    },
    "custom_validators": {
        "continue_final_message": {
            "validator_type": "value",
            "param_name": "add_generation_prompt", 
            "target_value": [false],
            "error_msg": "`add_generation_prompt` must equal False to use `continue_final_message`"
        }
    }
}